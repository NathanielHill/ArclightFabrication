{
  "name": "arclightfabrication.com",
  "version": "1.0.0",
  "description": "arclightfabrication.com website",
  "private": true,
  "repository": "https://github.com/NathanielHill/ArclightFabrication",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 next build",
    "build": "next build",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "deploy-from-travis": "next build && next export && cp -R static/rootAssets/* out/ && now out/ -n arclightfabrication --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "deploy": "next build && next export && cp -R static/rootAssets/* out/ && now out/ -n arclightfabrication && now alias",
    "dev:dynamic": "cross-env NODE_ENV=development node --inspect server.js",
    "dev:static": "next build && next export && cp -R static/rootAssets/* out/ && serve",
    "dev": "npm-run-all -p dev:dynamic open-browser",
    "export": "next export",
    "open-browser": "sleep 3 && opener http://localhost:3000",
    "sloc": "sloc pages/ src/ *.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "jest"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "React"
    ],
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard",
      "jest --bail --findRelatedTests"
    ]
  },
  "keywords": [],
  "author": "Nathaniel Hill <nata@goguna.com>",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.3.2",
    "apollo-link-http": "^1.5.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-wildcard": "^5.0.0",
    "cross-env": "^5.1.6",
    "express": "^4.16.4",
    "graphcms-image": "^1.1.0-beta4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "polished": "^2.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.4.0",
    "react-flip-move": "^3.0.3",
    "react-google-recaptcha": "^1.0.4",
    "react-modal": "^3.4.5",
    "react-slick": "^0.23.2",
    "react-tabs": "^2.2.2",
    "styled-components": "^4.0.3",
    "theming": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "coveralls": "^3.0.1",
    "husky": "^1.1.2",
    "jest": "^23.1.0",
    "lint-staged": "^8.0.4",
    "npm-run-all": "^4.1.3",
    "opener": "^1.4.3",
    "react-test-renderer": "^16.4.0",
    "regenerator-runtime": "^0.12.1",
    "serve": "^10.0.2",
    "sloc": "^0.2.0",
    "standard": "^12.0.1",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
