{
  "name": "arclightfabrication",
  "version": "1.0.0",
  "description": "ArclightFabrication.com Website",
  "private": true,
  "repository": "https://github.com/NathanielHill/ArclightFabrication",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "coveralls": "nyc --instrument=false --source-map=false report --temp-directory=./coverage --reporter=text-lcov | coveralls",
    "dev": "node --inspect server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "export": "next export",
    "analyze": "cross-env ANALYZE=1 next build",
    "deploy": "next build && next export && cp assets/* out/ && now out/ -n arclightfabrication && now alias",
    "precommit": "lint-staged && yarn test"
  },
  "now": {
    "alias": "staging.arclightfabrication.com",
    "env": {
      "ANALYTICS_TOKEN": "UA-119962353-1",
      "ANALYTICS_URL": "https://www.googletagmanager.com/gtag/js?id=",
      "INSTAGRAM_TOKEN": "553972643.2f2f131.3de3f2f99fc74647b7661167b32f1d55",
      "INSTAGRAM_URL": "https://api.instagram.com/v1/users/self/media/recent/?access_token=",
      "RECAPTCHA_TOKEN": "6LcO2zIUAAAAAALve9kpN_tTDGLIFFDVeLNj9DVV",
      "GRAPHCMS_API_URL": "https://api.graphcms.com/simple/v1/arclight"
    }
  },
  "babel": {
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "development": {
        "presets": [
          "next/babel"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "React"
    ],
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "keywords": [],
  "author": "Nathaniel Hill <nata@goguna.com>",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link-http": "^1.5.4",
    "babel-plugin-styled-components": "^1.5.1",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "graphcms-image": "^1.1.0-beta4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.0.3",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-async-script": "^0.9.1",
    "react-dom": "^16.4.0",
    "react-flip-move": "^3.0.2",
    "react-google-recaptcha": "^0.11.1",
    "react-modal": "^3.4.5",
    "react-slick": "^0.23.1",
    "react-tabs": "^2.2.2",
    "styled-components": "^3.3.0",
    "theming": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "coveralls": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.1.3",
    "now": "^11.2.1",
    "react-test-renderer": "^16.4.0",
    "regenerator-runtime": "^0.11.1",
    "standard": "^11.0.1",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
